        new_macro8: new_macro8 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&none &kp LS(M) &kp A &kp K &kp E &kp SPACE &kp F &kp O &kp R &kp M &kp A &kp L &kp COMMA &kp SPACE &kp P &kp O &kp L &kp I &kp S &kp H &kp COMMA &kp SPACE &kp S &kp L &kp I &kp G &kp H &kp T &kp L &kp Y &kp SPACE &kp P &kp O &kp L &kp I &kp T &kp E &kp RET>;
