            label = "attendingFR";
        };

        new_macro4: new_macro4 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&none &kp LS(A) &kp C &kp K &kp N &kp O &kp W &kp L &kp E &kp D &kp G &kp E &kp D &kp DOT &kp SPACE &kp LS(P) &kp L &kp E &kp A &kp S &kp E &kp SPACE &kp R &kp E &kp F &kp E &kp R &kp SPACE &kp T &kp O &kp SPACE &kp T &kp H &kp E &kp SPACE &kp A &kp T &kp T &kp A &kp C &kp H &kp E &kp D &kp SPACE &kp L &kp I &kp S &kp T &kp SPACE &kp O &kp F &kp SPACE &kp LS(F) &kp LS(R) &kp S &kp SPACE &kp F &kp O &kp R &kp SPACE &kp P &kp R &kp O &kp C &kp E &kp S &kp S &kp I &kp N &kp G &kp SPACE &kp O &kp F &kp SPACE &kp W &kp O &kp R &kp K &kp I &kp N &kp G &kp SPACE &kp P &kp E &kp R &kp M &kp I &kp T &kp DOT &kp SPACE &kp LS(T) &kp H &kp A &kp N &kp K &kp SPACE &kp Y &kp O &kp U &kp DOT>;
            label = "NEW_MACRO4";
        };

        new_macro5: new_macro5 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&none &kp N1 &kp X &kp SPACE &kp LS(O) &kp LS(N) &kp LS(U) &kp SPACE &kp M &kp O &kp D &kp E &kp M &kp RET &kp N1 &kp X &kp SPACE &kp LS(T) &kp E &kp L &kp S &kp E &kp T &kp SPACE &kp RET &kp N1 &kp X &kp SPACE &kp LS(I) &kp LS(O) &kp LS(O) &kp RET &kp N1 &kp X &kp SPACE &kp LS(S) &kp LS(C) &kp MINUS &kp LS(S) &kp LS(C) &kp SPACE &kp P &kp A &kp T &kp C &kp H &kp C &kp O &kp R &kp D &kp RET &kp N1 &kp X &kp SPACE &kp LS(L) &kp LS(C) &kp MINUS &kp LS(S) &kp LS(C) &kp SPACE &kp P &kp A &kp T &kp C &kp H &kp C &kp O &kp R &kp D &kp RET &kp N4 &kp X &kp SPACE &kp LS(S) &kp LS(C) &kp SPACE &kp C &kp O &kp N &kp N &kp E &kp C &kp T &kp O &kp R>;
            label = "SMFTTX";
        };

        new_macro6: new_macro6 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&none &kp N1 &kp X &kp SPACE &kp LS(O) &kp LS(N) &kp LS(U) &kp SPACE &kp M &kp O &kp D &kp E &kp M &kp RET &kp N1 &kp X &kp SPACE &kp LS(T) &kp E &kp L &kp S &kp E &kp T &kp SPACE &kp RET &kp N1 &kp X &kp SPACE &kp LS(I) &kp LS(O) &kp LS(O) &kp RET &kp N1 &kp X &kp SPACE &kp LS(S) &kp LS(C) &kp MINUS &kp LS(S) &kp LS(C) &kp SPACE &kp P &kp A &kp T &kp C &kp H &kp SPACE &kp C &kp O &kp R &kp D &kp SPACE &kp RET &kp N4 &kp X &kp SPACE &kp LS(S) &kp LS(C) &kp SPACE &kp C &kp O &kp N &kp N &kp E &kp C &kp T &kp O &kp R &kp RET>;
            label = "FTTXbundle";
        };

        new_macro7: new_macro7 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp N1 &kp X &kp SPACE &kp P &kp R &kp O &kp L &kp I &kp N &kp K &kp SPACE &kp M &kp O &kp D &kp E &kp M &kp RET &kp N1 &kp X &kp SPACE &kp T &kp E &kp L &kp S &kp E &kp T &kp RET &kp N1 &kp X &kp SPACE &kp LS(C) &kp LS(B) &kp LS(K)>;
