        new_macro9: new_macro9 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&none &kp LS(A) &kp C &kp K &kp N &kp O &kp W &kp L &kp E &kp D &kp G &kp E &kp D &kp DOT &kp SPACE &kp LS(W) &kp E &kp SPACE &kp W &kp I &kp L &kp L &kp SPACE &kp P &kp R &kp O &kp C &kp E &kp E &kp D &kp SPACE &kp W &kp I &kp T &kp H &kp SPACE &kp S &kp C &kp H &kp E &kp D &kp U &kp L &kp I &kp N &kp G &kp SPACE &kp T &kp H &kp E &kp SPACE &kp D &kp I &kp S &kp P &kp A &kp T &kp C &kp H &kp SPACE &kp O &kp N &kp C &kp E &kp SPACE &kp A &kp N &kp SPACE &kp LS(F) &kp LS(R) &kp SPACE &kp B &kp E &kp C &kp O &kp M &kp E &kp S &kp SPACE &kp A &kp V &kp A &kp I &kp L &kp A &kp B &kp L &kp E &kp DOT &kp SPACE &kp LS(T) &kp H &kp A &kp N &kp K &kp SPACE &kp Y &kp O &kp U &kp DOT &kp SPACE &kp RET>;
            label = "NEW_MACRO9";
        };

        new_macro10: new_macro10 {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&none &kp LS(S) &kp H &kp A &kp R &kp I &kp N &kp G &kp SPACE &kp R &kp E &kp M &kp A &kp R &kp K &kp S &kp SPACE &kp F &kp R &kp O &kp M &kp SPACE &kp LS(F) &kp LS(R) &kp SPACE &kp B &kp E &kp L &kp O &kp W &kp DOT &kp RET &kp RET &kp LS(A) &kp C &kp T &kp I &kp O &kp N &kp SPACE &kp T &kp A &kp K &kp E &kp N &kp COLON &kp RET &kp RET &kp LS(M) &kp A &kp T &kp E &kp R &kp I &kp A &kp L &kp S &kp SPACE &kp U &kp S &kp E &kp D &kp COLON &kp RET &kp RET &kp LS(F) &kp A &kp C &kp I &kp L &kp I &kp T &kp Y &kp SPACE &kp U &kp S &kp E &kp D &kp COLON &kp RET &kp RET &kp LS(A) &kp S &kp S &kp I &kp G &kp N &kp E &kp D &kp SPACE &kp LS(F) &kp LS(R) &kp COLON &kp RET &kp RET &kp RET>;
            label = "NEW_MACRO10";
        };
    };

    keymap {
        compatible = "zmk,keymap";

        Base {
            display-name = " QWE";
            bindings = <
&kp ESCAPE           &kp Q  &kp W  &kp E                   &kp R        &kp T                  &kp Y                  &kp U                 &kp I             &kp O             &kp P     &kp BACKSPACE
&mt LCTRL TAB        &kp A  &kp S  &kp D                   &lt 2 F      &kp G                  &kp H                  &hml RIGHT_CONTROL J  &hml RIGHT_GUI K  &hml RIGHT_ALT L  &kp SEMI  &kp SQT
&mt LEFT_SHIFT CAPS  &kp Z  &kp X  &kp C                   &kp V        &kp B                  &kp N                  &kp M                 &kp COMMA         &kp DOT           &kp FSLH  &kp RIGHT_SHIFT
                                   &mt LEFT_ALT BACKSPACE  &lt 2 ENTER  &lt 1 SPACE            &mt RIGHT_SHIFT SPACE  &lt 3 ENTER           &lt 1 BACKSPACE
            >;

            sensor-bindings = <&scroll_encoder>;
        };

        NAV {
            bindings = <
&trans         &new_macro14    &kp HOME            &kp UP_ARROW    &kp END          &kp P                          &trans          &mkp LCLK           &mmv MOVE_UP    &mkp RCLK            &msc SCRL_UP     &trans
&kp LC(PLUS)   &msc SCRL_UP    &kp LEFT            &kp DOWN        &kp RIGHT        &kp O                          &trans          &mmv MOVE_LEFT      &mmv MOVE_DOWN  &mmv MOVE_RIGHT      &msc SCRL_DOWN   &trans
&kp LC(MINUS)  &msc SCRL_DOWN  &kp LA(LEFT_ARROW)  &msc SCRL_LEFT  &msc SCRL_RIGHT  &kp LA(RIGHT_ARROW)            &msc SCRL_LEFT  &kp LA(LEFT_ARROW)  &trans          &kp LA(RIGHT_ARROW)  &msc SCRL_RIGHT  &trans
                                                   &trans          &trans           &trans                         &mkp LCLK       &mkp RCLK           &trans
            >;

            sensor-bindings = <&rgb_encoder>;
            label = "NAV";
        };

        NUM_L {
            display-name = "NUM-L";
            bindings = <
&kp COLON  &kp ASTERISK  &kp N7  &kp N8  &kp N9        &kp PLUS               &kp C_MUTE             &kp C_PREVIOUS         &kp C_PLAY_PAUSE  &kp C_NEXT            &trans  &trans
&kp DOT    &kp SLASH     &kp N4  &kp N5  &kp N6        &kp MINUS              &trans                 &kp RA(C_VOLUME_DOWN)  &trans            &kp RA(C_VOLUME_UP)   &trans  &trans
&trans     &kp NUMBER_0  &kp N1  &kp N2  &kp N3        &kp EQUAL              &trans                 &kp C_BRIGHTNESS_DEC   &trans            &kp C_BRIGHTNESS_INC  &trans  &trans
                                 &mo 4   &lt 1 DELETE  &new_macro2            &mt RIGHT_SHIFT SPACE  &lt 1 BACKSPACE        &kp RIGHT_ALT
            >;

            sensor-bindings = <&inc_dec_kp C_VOLUME_UP C_VOLUME_DOWN>;
            label = "num_left";
        };

        NSF {
            bindings = <
&kp TILDE  &kp EXCLAMATION  &kp AT_SIGN  &kp HASH  &kp DOLLAR  &kp PERCENT            &kp CARET  &kp AMPERSAND  &kp ASTERISK  &kp LEFT_PARENTHESIS  &kp RIGHT_PARENTHESIS  &kp UNDERSCORE
&kp GRAVE  &kp N1           &kp N2       &kp N3    &kp N4      &kp N5                 &kp N6     &kp N7         &kp N8        &kp N9                &kp N0                 &kp MINUS
&kp F1     &kp F2           &kp F3       &kp F4    &kp F5      &kp F6                 &kp F7     &kp F8         &kp F9        &kp F10               &kp F11                &kp F12
                                         &trans    &trans      &trans                 &trans     &trans         &trans
            >;

            label = "NSF";
        };

        Macros {
            bindings = <
&new_macro5  &new_macro1  &kp LC(LS(AMPERSAND))  &new_macro15  &new_macro   &ext_power EP_ON             &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4  &trans
&new_macro6  &new_macro9  &new_macro4            &new_macro3   &trans       &ext_power EP_OFF            &trans        &trans        &trans        &trans        &trans        &trans
&new_macro7  &trans       &kp LC(UNDERSCORE)     &new_macro10  &new_macro8  &out OUT_TOG                 &out OUT_TOG  &out OUT_BLE  &trans        &trans        &trans        &bt BT_CLR_ALL
                                                 &trans        &trans       &out OUT_BLE                 &trans        &trans        &trans
            >;

